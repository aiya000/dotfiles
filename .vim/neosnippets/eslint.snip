snippet eslint_disable_lines
    /* eslint-disable ${1:name} */
    ${0}
    /* eslint-enable $1 */

snippet eslint_disable
options indent
abbr /* eslint ${1:name}: 0 */
    /* eslint ${1:name}: 0 */

snippet eslint_enable_warn
options indent
abbr /* eslint ${1:name}: 1 */
    /* eslint ${1:name}: 1 */

snippet eslint_enable_error
options indent
abbr /* eslint ${1:name}: 2 */
    /* eslint ${1:name}: 2 */

snippet eslint_ignore_next
abbr // eslint-disable-next-line ${0}
    // eslint-disable-next-line ${0}

snippet eslint_ignore_previous
alias eslint_ignore
abbr // eslint-disable-line ${0}
    // eslint-disable-line ${0}

snippet eslint_ignore_no_unused_vars
abbr // eslint-disable-line no-unused-vars
    // eslint-disable-line no-unused-vars

snippet eslint_ignore_no_unused_expressions
abbr // eslint-disable-line no-unused-expressions
    // eslint-disable-line no-unused-expressions

snippet eslint_ignore_typescript_eslint_explicit_module_boundary_types
abbr // eslint-disable-line @typescript-eslint/explicit-module-boundary-types
    // eslint-disable-line @typescript-eslint/explicit-module-boundary-types

snippet eslint_ignore_typescript_eslint_no_explicit_any
abbr // eslint-disable-line @typescript-eslint/no-explicit-any
    // eslint-disable-line @typescript-eslint/no-explicit-any

snippet eslint_ignore_next_any
alias eslint_ignore_for_type_check_function
abbr // eslint-disable-next-line @typescript-eslint/explicit-module-boundary-types, @typescript-eslint/no-explicit-any
    // eslint-disable-next-line @typescript-eslint/explicit-module-boundary-types, @typescript-eslint/no-explicit-any
