snippet function
alias   func
abbr    function! () abort
options head
    function! ${1:#:name}(${2:#:args}) abort
        ${0:TARGET}
    endfunction

snippet if
options head
    if ${1:#:condition}
        ${0:TARGET}
    endif

snippet for
options head
    for ${1:#:var} in ${2:iter}
        ${0:TARGET}
    endfor

snippet try
options head
    try
        ${1}
    catch
        ${2}
    endtry

snippet vimconsolelog
alias log
options word
    VimConsoleLog
