snippet func
    func ${1:#:funcName}(${2})${3:#:int} {
        ${0:#:return}
    }

snippet print
    fmt.Print(${0})

snippet import
alias   imp
    import (
        "${1:fmt}"
        ${2}
    )
    ${0}

snippet else
alias   el
    else {
        ${0}
    }

snippet fprint
    fmt.Fprint(${1:os.Stderr}, ${2:err})

snippet fprintln
    fmt.Fprintln(${1:os.Stderr}, ${2:err})
