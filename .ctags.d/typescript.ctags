--langdef=typescript
--langmap=typescript:.ts
--regex-typescript=/^[ \t]*(export[ \t]*)?(declare[ \t]*)?(default[ \t]*)?class[ \t]+([a-zA-Z0-9_]+)/\4/c,classes/
--regex-typescript=/^[ \t]*(export[ \t]*)?(declare[ \t]*)?(default[ \t]*)?module[ \t]+([a-zA-Z0-9_]+)/\4/n,modules/
--regex-typescript=/^[ \t]*(export[ \t]*)?(declare[ \t]*)?(default[ \t]*)?(async[ \t]*)?function[ \t]+([a-zA-Z0-9_]+)/\5/f,functions/
--regex-typescript=/^[ \t]*(export[ \t]*)?(declare[ \t]*)?(default[ \t]*)?var[ \t]+([a-zA-Z0-9_]+)/\4/v,variables/
--regex-typescript=/^[ \t]*(export[ \t]*)?(declare[ \t]*)?(default[ \t]*)?(async[ \t]*)?(public|private)?(async[ \t]*)?[ \t]+(static)?[ \t]*([a-zA-Z0-9_]+)/\8/m,members/
--regex-typescript=/^[ \t]*(export[ \t]*)?(declare[ \t]*)?(default[ \t]*)?interface[ \t]+([a-zA-Z0-9_]+)/\4/i,interfaces/
--regex-typescript=/^[ \t]*(export[ \t]*)?(declare[ \t]*)?(default[ \t]*)?enum[ \t]+([a-zA-Z0-9_]+)/\4/e,enums/
--regex-typescript=/^[ \t]*(export[ \t]*)?(declare[ \t]*)?(default[ \t]*)?type[ \t]+([a-zA-Z0-9_]+)/\4/t,typealias/
--regex-typescript=/^[ \t]*(export[ \t]*)?(declare[ \t]*)?(default[ \t]*)?namespace[ \t]+([a-zA-Z0-9_]+)/\4/N,namespace/
--regex-typescript=/^[ \t]*import[ \t]+([a(default[ \t]*)?-zA-Z0-9_]+)/\1/I,imports/
