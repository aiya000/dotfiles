#!/bin/bash
configure='./configure --prefix=/usr/local/ --enable-fail-if-missing --enable-gui=yes --enable-multibyte=yes --enable-perlinterp=yes --enable-pythoninterp=yes --enable-rubyinterp=yes --enable-luainterp=dynamic --with-lua-prefix=/usr --with-luajit --enable-cscope=yes --enable-gpm --enable-cscope --enable-fontset --with-features=huge --with-x --with-compiledby=aiya000 --enable-terminal'

if [ -f ./configure ] ; then
    echo "$configure"
    eval "$configure"
else
    echo './configure cannot be found'
fi
